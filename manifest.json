{
  "name": "Eye Health Extension",
  "version": "0.1.0",
  "description": "Keeping your eyes healthy with reminding you to take off of your eyes from monitor every 20 inutes",
  "permissions": ["storage", "tabs"],
  "host_permissions": ["https://*.youtube.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_title": "Eye Health Extension",
    "default_popup": "popup.html"
  },
  "manifest_version": 3
}
